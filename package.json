{
  "name": "re-pack",
  "version": "0.0.3",
  "description": "Minimalistic configuration for a complete and configurable react, webpack and firebase environment for development and production builds.",
  "private": false,
  "homepage": "https://github.com/john-d-pelingo/re-pack#readme",
  "author": "John Darryl Pelingo <johndarrylpelingo@gmail.com> (https://github.com/john-d-pelingo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/john-d-pelingo/re-pack/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/john-d-pelingo/re-pack.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks --progress",
    "clean": "del-cli public",
    "deploy": "firebase deploy",
    "lint": "eslint -c .eslintrc.yml *.js src server",
    "prebuild": "run-s clean lint test",
    "predeploy": "npm run build",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start": "npm run server:dev",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "dependencies": {
    "autoprefixer": "6.6.1",
    "babel-polyfill": "6.20.0",
    "expect": "1.20.2",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "firebase": "3.6.4",
    "history": "4.6.0",
    "html-webpack-plugin": "2.26.0",
    "morgan": "1.7.0",
    "prop-types": "15.5.7",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-ga": "2.1.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "5.0.3",
    "react-router": "4.0.0",
    "react-router-dom": "4.0.0",
    "react-router-redux": "5.0.0-alpha.3",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "webpack": "1.14.0",
    "webpack-md5-hash": "0.0.5",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "babel-core": "6.4.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.4.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.22.0",
    "cross-env": "3.2.4",
    "css-loader": "0.26.4",
    "del-cli": "0.2.1",
    "eslint": "3.17.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.10.0",
    "firebase-tools": "3.5.0",
    "jasmine-core": "2.5.2",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.8.1",
    "minx": "0.4.14",
    "node-env-file": "0.1.8",
    "node-sass": "4.5.0",
    "nodemon": "1.11.0",
    "npm-run-all": "3.1.2",
    "postcss-loader": "1.3.3",
    "react-addons-test-utils": "15.4.2",
    "sass-loader": "4.1.1",
    "sinon": "1.17.7",
    "style-loader": "0.13.2",
    "webpack-dev-server": "1.16.3"
  }
}
